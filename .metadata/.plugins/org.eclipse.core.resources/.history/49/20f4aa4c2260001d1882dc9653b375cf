package com.ojas.entity;

public class RegularEmployee extends Employee {
	
	private float Salary;
	private int bonus;
	
	public float getSalary() {
		return Salary;
	}
	public void setSalary(float salary) {
		Salary = salary;
	}
	public int getBonus() {
		return bonus;
	}
	public void setBonus(int bonus) {
		this.bonus = bonus;
	}
	
}
